@{
    ViewData["Title"] = "Home Page";
}

@model IEnumerable<TechnologyCorporations.Models.TechCorporationModel>

<h2>Tech Corporations</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table">
    <tr>
        <th></th>
        <th>Corporation name</th>
        <th>Customer service</th>
        <th>Founders</th>
        <th>Headquarters</th>
        <th>Founded</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <img src=@item.ImageSource title="Image of @item.CorporationName" width="100" height="100" />
            </td>
            <td>@item.CorporationName</td>
            <td>@item.CustomerService</td>
            <td>@item.Founders</td>
            <td>@item.Headquarters</td>
            <td>@item.Founded</td>
